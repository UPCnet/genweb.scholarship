<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="genweb.scholarship">

    <body>
        <metal:content-core fill-slot="content-core">
        <metal:block define-macro="content-core">

        <!-- <p tal:content="view/getTitle"></p> -->
        <div class="row-fluid">
            <div class="span12">
                <div tal:replace="structure context/organism/output|nothing"/>
                <div tal:replace="structure context/recipients/output|nothing"/>
                <div tal:replace="structure context/general_requirements/output|nothing"/>
                <div tal:replace="structure context/academic_requirements/output|nothing"/>
                <div tal:replace="structure context/economic_requirements/output|nothing"/>
                <div tal:replace="structure context/incompatibilities/output|nothing"/>
                <tal:block condition="context/start_date">
                    <div tal:content="python:context.start_date.strftime('%d/%m/%Y')"/>
                </tal:block>
                <tal:block condition="context/deadline">
                    <div tal:content="python:context.deadline.strftime('%d/%m/%Y')"/>
                </tal:block>
                <div tal:replace="structure context/submission/output|nothing"/>
                <div tal:replace="structure context/documentation/output|nothing"/>
                <div tal:replace="structure context/amount|nothing"/>
                <div tal:replace="structure context/additional_amount|nothing"/>
                <div tal:replace="structure context/duration/output|nothing"/>
                <div tal:replace="structure context/payment/output|nothing"/>
                <div tal:replace="structure context/beneficiaries/output|nothing"/>
                <div tal:replace="structure context/criteria/output|nothing"/>
                <tal:block condition="context/award_date">
                    <div tal:content="python:context.award_date.strftime('%d/%m/%Y')"/>
                </tal:block>
                <div tal:replace="structure context/award_resolution/output|nothing"/>
                <div tal:replace="structure context/allegations/output|nothing"/>
                <div tal:replace="structure context/regulations/output|nothing"/>
            </div>
        </div>
        <div class="row-fluid">
            <h3>Si voleu contactar amb nosaltres:</h3>
            <div class="row-fluid">
                <div class="span12">
                    <h4>Atenci√≥ on-line:</h4>
                    <a class="external-link" href="https://demana.upc.edu/sga/"
                    target="_blank" title="Demana UPC">
                        <img src="http://localhost:8080/166/sgaprova/ca/shared/imatges/banner/banner-demana-sga.gif"
                             alt="Demana SGA" class="image-inline"
                             title="Demana SGA">
                    </a>
                </div>
            </div>
        </div>

        </metal:block>
    </metal:content-core>

</body>
</html>
